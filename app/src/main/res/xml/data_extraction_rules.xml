<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- 禁止云备份敏感数据 -->
        <exclude domain="database" />
        <exclude domain="sharedpref" />
        <exclude domain="file" path="images/" />
        <exclude domain="file" path="audio/" />
    </cloud-backup>
    <device-transfer>
        <!-- 设备传输时也排除敏感数据 -->
        <exclude domain="database" />
        <exclude domain="sharedpref" />  
        <exclude domain="file" path="images/" />
        <exclude domain="file" path="audio/" />
    </device-transfer>
</data-extraction-rules>